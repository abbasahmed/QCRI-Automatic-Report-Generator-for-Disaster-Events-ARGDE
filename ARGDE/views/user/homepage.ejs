<script>
$(document).ready(function(){
  var i = 0;
  var txt = 'where data comes to life';
  var speed = 50;

  function typeWriter() {
    if (i < txt.length) {
      document.getElementById("tagline").innerHTML += txt.charAt(i);
      i++;
      setTimeout(typeWriter, speed);
    }
  }
  setTimeout(function(){typeWriter();},2000);
});
</script>
