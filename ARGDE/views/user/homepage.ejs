<% title='A.R.G.D.E' %>
<div class="container" id="web_app_home">
  <div id="web_app_home_heading">
    <br>
    A.R.G.D.E.
  </div>
  <strong>
    <div id="tagline">
    </div>
  </strong>
</div>
<style>
  body{
    background-image: url("https://wallpaperstudio10.com/static/wpdb/wallpapers/3840x2160/176410.jpg");
    background-repeat: no-repeat;
    background-size: cover;
    font-family: 'Poppins', sans-serif;
  }
</style>
<script>
$(document).ready(function(){
  var i = 0;
  var txt = 'where data comes to life.';
  var speed = 50;

  function typeWriter() {
    if (i < txt.length) {
      document.getElementById("tagline").innerHTML += txt.charAt(i);
      i++;
      setTimeout(typeWriter, speed);
    }
  }
  setTimeout(function(){typeWriter();},1000);
});
</script>
